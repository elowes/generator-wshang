{
  "presets": [
    [
      "@babel/env", {
        "targets": {
          "browsers": "> 1%"
        },
        "useBuiltIns": "usage"
      }
    ],
    [
      "@babel/preset-react"
    ]
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/syntax-dynamic-import",
    [
      "@babel/plugin-proposal-decorators", 
      { 
        "legacy": true 
      }
    ],
    "@babel/plugin-proposal-class-properties",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": ["transform-react-remove-prop-types"]
    }
  }
}